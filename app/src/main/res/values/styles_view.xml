<resources>

    <style name="NumButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/text_size_32</item>
        <item name="android:textColor">#282828</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/shape_rectangle_stroke_b4b4b4</item>
    </style>

    <style name="NumButton_1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:textSize">@dimen/text_size_32</item>
        <item name="android:textColor">#282828</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/shape_stroke_b4b4b4</item>
    </style>

    <style name="NumButton_2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">94dp</item>
        <item name="android:textSize">@dimen/text_size_32</item>
        <item name="android:textColor">#282828</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/shape_stroke_b4b4b4</item>
    </style>

    <style name="NumButton_3" parent="NumButton">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/shape_stroke_b4b4b4</item>
    </style>

    <style name="InputGoodsTitle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#282828</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="InputGoodsItem">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black_444444</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="ShouYinBottomButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">54dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/shape_shouyin_button</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#FF4F4F4F</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="OrderDesTextView1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FF282828</item>
        <item name="android:textSize">@dimen/text_size_18</item>
    </style>

    <style name="OrderDesTextView1_6" parent="OrderDesTextView1">
        <item name="android:layout_marginTop">6dp</item>
    </style>

    <style name="OrderDesTextView2" parent="OrderDesTextView1">
        <item name="android:gravity">center</item>
    </style>

    <style name="OrderDesTextView3">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FF282828</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/text_size_18</item>
    </style>

    <style name="duiZhangTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:textColor">#FF282828</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/text_size_18</item>
    </style>

    <style name="duiZhangTextView1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:textColor">#FF282828</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/text_size_18</item>
    </style>

    <style name="TabLayoutTextStyle">
        <item name="android:textSize">@dimen/text_size_24</item>
    </style>

    <style name="SpinnerStyle" parent="android:style/Widget.ListView.DropDown">
        <item name="android:divider">#ff0000</item>
        <item name="android:dividerHeight">1dp</item>
    </style>

    <style name="SpinnerTheme" parent="@android:style/Theme.Holo.Light">
        <item name="android:dropDownListViewStyle">@style/SpinnerStyle</item>
    </style>

    <style name="printItemTextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#FF444444</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="printBarcodeTitleTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#444</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="printBarcodeValueTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">#444</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textScaleX">1.5</item>
    </style>

    <style name="accountCheckTextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/shape_rectangle_stroke_000000</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#FF444444</item>
        <item name="android:textSize">22sp</item>
    </style>
</resources>
